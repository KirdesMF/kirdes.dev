{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "pnpm build:css && pnpm build:remix",
    "build:css": "pnpm generate:css",
    "build:remix": "remix build",
    "dev": "concurrently \"pnpm dev:css\" \"pnpm dev:remix\"",
    "dev:css": "pnpm generate:css --watch",
    "dev:remix": "remix dev",
    "generate:css": "unocss './app/**/*.{js,ts,jsx,tsx}' -o ./app/styles/uno.css",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@remix-run/node": "^1.13.0",
    "@remix-run/react": "^1.13.0",
    "@remix-run/vercel": "^1.13.0",
    "@unocss/reset": "^0.50.1",
    "@vercel/node": "^2.6.2",
    "gsap": "^3.11.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.13.0",
    "@remix-run/eslint-config": "^1.13.0",
    "@remix-run/serve": "^1.13.0",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@unocss/cli": "^0.50.1",
    "@unocss/eslint-config": "^0.50.1",
    "clsx": "^1.2.1",
    "concurrently": "^7.6.0",
    "eslint": "^8.27.0",
    "typescript": "^4.8.4",
    "unocss": "^0.50.1"
  },
  "engines": {
    "node": ">=14"
  }
}
